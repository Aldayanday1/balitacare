# BalitaCare Environment Configuration
# Copy this file to .env and fill with your actual values
# DO NOT commit .env file to version control

# Application Configuration
APP_NAME=BalitaCare Plus
APP_VERSION=1.0.0
ENVIRONMENT=development

# Database Configuration
DATABASE_NAME=balita.db
DATABASE_VERSION=1

# Notification Configuration
NOTIFICATION_CHANNEL_ID=download_channel_id
NOTIFICATION_CHANNEL_NAME=Unduhan Berhasil
NOTIFICATION_ICON=@mipmap/ic_launcher

# Export Configuration
EXPORT_FOLDER_NAME=BalitaCare_Exports
DEFAULT_FILE_PREFIX=balita_perkembangan

# API Configuration (if needed in future)
# API_BASE_URL=https://api.balitacare.id
# API_KEY=your_api_key_here
# API_SECRET=your_api_secret_here

# Firebase Configuration (if needed in future)
# FIREBASE_PROJECT_ID=your_project_id
# FIREBASE_API_KEY=your_api_key
# FIREBASE_APP_ID=your_app_id

# Contact Information (for support)
SUPPORT_EMAIL=support@balitacare.id
SUPPORT_TELEGRAM=@balitacare_support

# Feature Flags
ENABLE_ANALYTICS=false
ENABLE_CRASH_REPORTING=false
ENABLE_CLOUD_SYNC=false
